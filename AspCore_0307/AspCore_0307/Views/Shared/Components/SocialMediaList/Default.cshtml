@using EntityLayer.Concrete
@model List<SocialMedia>
<div class="social-nav" data-aos="fade-up" data-aos-delay="200">
    <nav role="navigation">
        <ul class="nav justify-content-left">

            @foreach (var item in Model)
            {
                <li class="nav-item"><a class="nav-link" href="@item.SocialUrl" target="_blank" title="@item.SocialMediaName"><i class="@item.İcon"></i><span class="menu-title sr-only">@item.SocialMediaName</span></a></li>
            }
        </ul>
    </nav>
</div>